# ============================================
# Laravel GitHub Pages Deployment - .gitignore
# For transcription.1oa.cc
# ============================================

# ==================
# Laravel & PHP
# ==================
*.log
.phpactor.json
.phpunit.result.cache
/auth.json
/vendor
composer.phar
Thumbs.db

# ==================
# Environment & Configuration
# ==================
.env
.env.backup
.env.production
.env.local
.env.*.local
!.env.example

# ==================
# Storage & Cache
# ==================
/storage/*.key
/storage/pail
/storage/logs/*
!/storage/logs/.gitkeep
/storage/framework/cache/data/*
!/storage/framework/cache/data/.gitkeep
/storage/framework/sessions/*
!/storage/framework/sessions/.gitkeep
/storage/framework/testing/*
!/storage/framework/testing/.gitkeep
/storage/framework/views/*
!/storage/framework/views/.gitkeep
/bootstrap/cache/*
!/bootstrap/cache/.gitkeep

# ==================
# Public Build Outputs
# ==================
/public/build
/public/hot
/public/storage

# ==================
# Node.js & Package Managers
# ==================
/node_modules
npm-debug.log
yarn-error.log
yarn.log
.pnpm-debug.log
.pnpm-store

# Lock files (choose one package manager)
# package-lock.json  # Uncomment if using npm
# yarn.lock          # Uncomment if using yarn
# pnpm-lock.yaml     # Uncomment if using pnpm

# ==================
# Testing
# ==================
/.phpunit.cache
coverage/
.phpunit.result.cache
/phpunit.xml  # Keep phpunit.xml in repo for CI, but you can ignore local overrides

# ==================
# IDE & Editors
# ==================
/.fleet
/.idea
/.nova
/.vscode
/.zed
*.swp
*.swo
*~
.project
.settings
.buildpath
*.sublime-project
*.sublime-workspace

# ==================
# Operating System
# ==================
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
desktop.ini

# ==================
# Laravel Specific
# ==================
Homestead.json
Homestead.yaml
/.vagrant

# ==================
# Database
# ==================
*.sqlite
*.sqlite-journal
/database/*.sqlite
/database/*.sqlite-journal

# ==================
# Temporary Files
# ==================
*.tmp
*.temp
nul

# ==================
# GitHub Pages Deployment
# ==================
# Static site build output (generated by our build script)
/public_html
/dist
/build
/.gh-pages

# Keep docs for GitHub
# But ignore old migration/analysis docs
MIGRATION_*.md
FRONTEND_ANALYSIS_REPORT.md
*-backup-original/

# ==================
# Project Specific
# ==================
# Keep these for reference but not in production
/HTML-ORIGINALS
/CONSTRUCT
